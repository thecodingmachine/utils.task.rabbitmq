{
    "name": "moufutils.task.rabitmq",
    "description": "This package is used to manage task in RabbitMq. It provides classes to create task in RabbitMq and consums it.",
    "keywords": ["session", "mouf"],
    "homepage": "https://github.com/thecodingmachine/utils.task.rabbitmq",
    "type": "mouf-library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre Vaidie",
            "email": "p.vaidie@thecodingmachine.com",
            "homepage": "http://mouf-php.com"
        }
    ],
    "require": {
    	"php": ">=5.3.0",
	    "mouf/utils.console": "~1.0",
	    "php-amqplib/php-amqplib": "2.6.*",
	    "guzzlehttp/guzzle": "~6.0@dev",
	    "league/tactician": "^1.0"
    },
    "autoload": {
        "psr-0": {
            "Mouf\\Utils\\Action": "src/",
            "Mouf\\Utils\\Session\\SessionManager": "src/"
        }
    },
    "extra": {
	     "mouf": {
		    "install" : [{
			    "type" : "class",
			    "class" : "Mouf\\Utils\\Patcher\\PatchInstaller",
			    "description": "Create the patchService instance."
			}]
		}
    }
}
